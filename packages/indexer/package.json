{
  "name": "@wofi/indexer",
  "version": "0.1.0",
  "type": "module",
  "main": "dist/src/index.js",
  "types": "dist/src/index.d.ts",
  "files": [
    "dist",
    "migrations",
    "README.md"
  ],
  "scripts": {
    "build": "tsc -b",
    "test": "npm run build && node --test dist/test/**/*.js",
    "typecheck": "tsc --noEmit",
    "lint": "echo \"no lint configured for @wofi/indexer\"",
    "sync": "node dist/src/cli.js sync --from-outbox",
    "backfill": "node dist/src/cli.js backfill",
    "replay": "node dist/src/cli.js replay",
    "migrate": "node dist/src/cli.js migrate",
    "seed": "node dist/src/seed.js"
  },
  "dependencies": {
    "@wofi/kernel": "file:../kernel",
    "pg": "^8.12.0"
  },
  "devDependencies": {
    "@types/node": "^20.11.17",
    "@types/pg": "^8.16.0",
    "pg-mem": "^2.9.0",
    "typescript": "^5.3.3"
  }
}
